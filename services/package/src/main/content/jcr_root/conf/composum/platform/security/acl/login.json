[
  {
    "description": "allows a 'walk through' the given paths for 'everyone'",
    "path": [
      "/libs",
      "/libs/composum",
      "/libs/composum/platform"
    ],
    "jcr:primaryType": "sling:Folder",
    "acl": {
      "principal": "everyone",
      "rule": {
        "grant": "jcr:read",
        "restrictions": {
          "rep:glob": ""
        }
      }
    }
  },
  {
    "description": "allows read access for 'everyone' at the given paths",
    "path": [
      "/libs/fonts",
      "/libs/jslibs",
      "/libs/composum/platform/public"
    ],
    "jcr:primaryType": "sling:Folder",
    "acl": {
      "principal": "everyone",
      "rule": {
        "grant": "jcr:read"
      }
    }
  }
]
